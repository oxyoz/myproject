<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@drawable/common_title_selector" >

    <RelativeLayout
        android:id="@+id/relativeLayout1"
        style="@style/title_Style"
        android:layout_width="match_parent"
        android:layout_height="@dimen/layout_height_50" >

        <Button
            android:id="@+id/common_back"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:background="@drawable/bt_back_selector"
            android:visibility="gone" />

        <Button
            android:id="@+id/bt_map"
            style="@style/mid_font_white_style"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:background="@null"
            android:textColor="@drawable/text_color_selector"
            android:textSize="@dimen/b18_font" />

        <TextView
            android:id="@+id/common_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_centerVertical="true"
            android:ellipsize="end"
            android:gravity="center"
            android:maxEms="10"
            android:singleLine="true"
            android:text=""
            android:textColor="@color/white"
            android:textSize="@dimen/b18_font" />

        <TextView
            android:id="@+id/common_bt"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginRight="5dp"
            android:layout_toLeftOf="@+id/bt_map"
            android:layout_toRightOf="@+id/button1"
            android:padding="5dp"
            android:ellipsize="end"
            android:gravity="center"
            android:singleLine="true"
            android:text=""
            android:textColor="@color/white"
            android:textSize="@dimen/text_order"
            android:visibility="gone" />

        <ImageView
            android:id="@+id/common_img"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginRight="5dp"
            android:layout_toLeftOf="@+id/bt_map"
            android:ellipsize="end"
            android:gravity="center"
            android:visibility="gone" />
    </RelativeLayout>

</RelativeLayout>