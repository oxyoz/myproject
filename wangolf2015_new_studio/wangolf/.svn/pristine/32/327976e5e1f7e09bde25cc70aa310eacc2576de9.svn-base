package me.wangolf.bean.usercenter;

import java.io.Serializable;
import java.util.ArrayList;

/**
 * 红包列表实体
 * 
 */
public class RedPackEntity implements Serializable {
	private static final long serialVersionUID = 1L;
	private int id;// "7",//红包ID
	private String price;// "50",//价值
	private int status;// "1",//0代表未分享1代表已分享
	private String addtime;// "2014-11-19 11:25:28",//消费时间
	private String info;// "活动消费"//消费内容
	private String web_app_uri;
	private String total_amount;
	private ArrayList<RedPackEntity> data;

	public int getId() {
		return id;
	}

	public void setId(int id) {
		this.id = id;
	}

	public String getPrice() {
		return price;
	}

	public void setPrice(String price) {
		this.price = price;
	}

	public int getStatus() {
		return status;
	}

	public void setStatus(int status) {
		this.status = status;
	}

	public String getAddtime() {
		return addtime;
	}

	public void setAddtime(String addtime) {
		this.addtime = addtime;
	}

	public String getInfo() {
		return info;
	}

	public void setInfo(String info) {
		this.info = info;
	}

	public ArrayList<RedPackEntity> getData() {
		return data;
	}

	public void setData(ArrayList<RedPackEntity> data) {
		this.data = data;
	}

	public String getTotal_amount() {
		return total_amount;
	}

	public void setTotal_amount(String total_amount) {
		this.total_amount = total_amount;
	}

	public String getWeb_app_uri() {
		return web_app_uri;
	}

	public void setWeb_app_uri(String web_app_uri) {
		this.web_app_uri = web_app_uri;
	}
}
