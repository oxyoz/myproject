package me.wangolf.bean.ball;

import java.io.Serializable;
import java.util.ArrayList;

/**
 * 球场详细实体
 * @author Administrator
 *
 */
public class BallInfoEntity implements Serializable{

	private static final long serialVersionUID = 1L;
	
	private String address ;// 球场详细地址
	private String ballimg ;// 图片路径
    private String ballname;//球场名称
	private String balltag ;//球场标签(山地球场 2007年开业 18洞72杆”,(数据组装)) ,
	private double distance ;// 距离 ,
	private int id;//球场ID
	private double price ;// 价格 ,
	private String packages;//套餐
	//private String specil;//特价
	private String latitude;//纬度
	private String longitude;//经度
	private String city;//所在城市
	private String book;//预定说明
	
	private String specilprice;//"500",//特价价格
	private String specilbTime;// "14:30",//特价开始时间
	private String specileTime;// "15:00",//特价结束时间
	private String specilcount;// "1", //0代表没有特价1代表有特价
	public String status;
	public String info;
    private String web_app_uri;

    public String getWeb_app_uri() {
        return web_app_uri;
    }

    public void setWeb_app_uri(String web_app_uri) {
        this.web_app_uri = web_app_uri;
    }

    private ArrayList<BallInfoEntity> data;
	private ArrayList<BallDistriEntity> distributors;
	public String getSpecilprice() {
		return specilprice;
	}
	public void setSpecilprice(String specilprice) {
		this.specilprice = specilprice;
	}
	public String getSpecilbTime() {
		return specilbTime;
	}
	public void setSpecilbTime(String specilbTime) {
		this.specilbTime = specilbTime;
	}
	public String getSpecileTime() {
		return specileTime;
	}
	public void setSpecileTime(String specileTime) {
		this.specileTime = specileTime;
	}
	public String getSpecilcount() {
		return specilcount;
	}
	public void setSpecilcount(String specilcount) {
		this.specilcount = specilcount;
	}
	
	public String getBook() {
		return book;
	}
	public void setBook(String book) {
		this.book = book;
	}
	public String getLatitude() {
		return latitude;
	}
	public void setLatitude(String latitude) {
		this.latitude = latitude;
	}
	public String getLongitude() {
		return longitude;
	}
	public void setLongitude(String longitude) {
		this.longitude = longitude;
	}
	public String getCity() {
		return city;
	}
	public void setCity(String city) {
		this.city = city;
	}
	public String getBallimg() {
		return ballimg;
	}
	public void setBallimg(String ballimg) {
		this.ballimg = ballimg;
	}
	public String getPackages() {
		return packages;
	}
	public void setPackages(String packages) {
		this.packages = packages;
	}
	/*public String getSpecil() {
		return specil;
	}
	public void setSpecil(String specil) {
		this.specil = specil;
	}*/
	private ArrayList<BallDistriEntity> distributorList;//分销商集合
	
	public ArrayList<BallDistriEntity> getDistributorList() {
		return distributorList;
	}
	public void setDistributorList(ArrayList<BallDistriEntity> distributorList) {
		this.distributorList = distributorList;
	}
	public BallInfoEntity() {
	}
	public String getAddress() {
		return address;
	}
	public String getBallimag() {
		return ballimg;
	}
	public String getBallname() {
		return ballname;
	}
	public String getBalltag() {
		return balltag;
	}
	public double getDistance() {
		return distance;
	}
	public int getId() {
		return id;
	}
	public double getPrice() {
		return price;
	}
	public void setAddress(String address) {
		this.address = address;
	}

	public void setBallimag(String ballimg) {
		this.ballimg = ballimg;
	}
    public void setBallname(String ballname) {
		this.ballname = ballname;
	}
    public void setBalltag(String balltag) {
		this.balltag = balltag;
	}
    public void setDistance(double distance) {
		this.distance = distance;
	}
    public void setId(int id) {
		this.id = id;
	}
    public void setPrice(double price) {
		this.price = price;
	}
	public String getStatus() {
		return status;
	}
	public void setStatus(String status) {
		this.status = status;
	}
	public String getInfo() {
		return info;
	}
	public void setInfo(String info) {
		this.info = info;
	}

	public ArrayList<BallInfoEntity> getData() {
		return data;
	}
	public void setData(ArrayList<BallInfoEntity> data) {
		this.data = data;
	}
	public ArrayList<BallDistriEntity> getDistributors() {
		return distributors;
	}
	public void setDistributors(ArrayList<BallDistriEntity> distributors) {
		this.distributors = distributors;
	}

}
