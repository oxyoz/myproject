package me.wangolf.bean.practice;

import java.io.Serializable;

public class PracDistriEntity implements Serializable {

    private static final long serialVersionUID = 1L;

    private int id;

    private String book;
    private int price_type;// 类型(0代表按会员收费，1代表按记球收费，2代表按记时收费)
    private String price;// 价格
    private String price_description;// 粒球”//价格说明
    private String price_tag;//记球
    private String rgname;//球场名称

    public int getId() {
        return id;
    }

    public void setId(int id) {
        this.id = id;
    }

    public String getBook() {
        return book;
    }

    public void setBook(String book) {
        this.book = book;
    }

    public int getPrice_type() {
        return price_type;
    }

    public void setPrice_type(int price_type) {
        this.price_type = price_type;
    }

    public String getPrice() {
        return price;
    }

    public void setPrice(String price) {
        this.price = price;
    }

    public String getPrice_description() {
        return price_description;
    }

    public void setPrice_description(String price_description) {
        this.price_description = price_description;
    }

    public String getPrice_tag() {
        return price_tag;
    }

    public void setPrice_tag(String price_tag) {
        this.price_tag = price_tag;
    }

    public String getRgname() {
        return rgname;
    }

    public void setRgname(String rgname) {
        this.rgname = rgname;
    }

}
