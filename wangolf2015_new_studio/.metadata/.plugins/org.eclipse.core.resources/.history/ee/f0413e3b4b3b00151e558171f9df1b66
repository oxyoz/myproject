package me.wangolf.test;

import java.io.Serializable;
import java.util.List;

public final class Practice implements Serializable, Cloneable {

	/**
	 * 
	 */
	private static final long serialVersionUID = 1L;

	private static Practice mPractice = new Practice();
	
	public final static Practice newPractice() throws CloneNotSupportedException
	{
		return (Practice) mPractice.clone();
	}
		
	private Practice(){}
	
    /**
     * status : 1
     * data :
     * info : 成功
     */
    private int status;
    
    private List<DataEntity> data;
    
    private String info;

    public void setStatus(int status) {
        this.status = status;
    }

    public void setData(List<DataEntity> data) {
        this.data = data;
    }

    public void setInfo(String info) {
        this.info = info;
    }

    public int getStatus() {
        return status;
    }

    public List<DataEntity> getData() {
        return data;
    }

    public String getInfo() {
        return info;
    }

    public static class DataEntity {
        /**
         * icon : http://www.wangolf.me/public/court/20150610142754350527913.jpg
         * has_message : 70个打位/2层
         * range_name : 香蜜湖高尔夫练习场
         * tee_floor_num : 2
         * tee_num : 70
         * city : 77
         * id : 8
         * distance : 12.0千米
         * min_price : 0
         * area : 705
         * address : 广东省-深圳市-福田区
         * province : 6
         * longitude : 114.039907
         * latitude : 22.555559
         * mobile : 075582722225
         */
        private String icon;
        private String has_message;
        private String range_name;
        private String tee_floor_num;
        private String tee_num;
        private String city;
        private String id;
        private String distance;
        private String min_price;
        private String area;
        private String address;
        private String province;
        private String longitude;
        private String latitude;
        private String mobile;

        public void setIcon(String icon) {
            this.icon = icon;
        }

        public void setHas_message(String has_message) {
            this.has_message = has_message;
        }

        public void setRange_name(String range_name) {
            this.range_name = range_name;
        }

        public void setTee_floor_num(String tee_floor_num) {
            this.tee_floor_num = tee_floor_num;
        }

        public void setTee_num(String tee_num) {
            this.tee_num = tee_num;
        }

        public void setCity(String city) {
            this.city = city;
        }

        public void setId(String id) {
            this.id = id;
        }

        public void setDistance(String distance) {
            this.distance = distance;
        }

        public void setMin_price(String min_price) {
            this.min_price = min_price;
        }

        public void setArea(String area) {
            this.area = area;
        }

        public void setAddress(String address) {
            this.address = address;
        }

        public void setProvince(String province) {
            this.province = province;
        }

        public void setLongitude(String longitude) {
            this.longitude = longitude;
        }

        public void setLatitude(String latitude) {
            this.latitude = latitude;
        }

        public void setMobile(String mobile) {
            this.mobile = mobile;
        }

        public String getIcon() {
            return icon;
        }

        public String getHas_message() {
            return has_message;
        }

        public String getRange_name() {
            return range_name;
        }

        public String getTee_floor_num() {
            return tee_floor_num;
        }

        public String getTee_num() {
            return tee_num;
        }

        public String getCity() {
            return city;
        }

        public String getId() {
            return id;
        }

        public String getDistance() {
            return distance;
        }

        public String getMin_price() {
            return min_price;
        }

        public String getArea() {
            return area;
        }

        public String getAddress() {
            return address;
        }

        public String getProvince() {
            return province;
        }

        public String getLongitude() {
            return longitude;
        }

        public String getLatitude() {
            return latitude;
        }

        public String getMobile() {
            return mobile;
        }
    }
	
	
}
