package me.wangolf.bean.ball;

import java.io.Serializable;
import java.util.ArrayList;

public class BallTagDetailEntity implements Serializable{
	private static final long serialVersionUID = 1L;
	private long area; // 面积  
	
	 private String bkdata; // 球场数据  
	private String createtime; // 建立时间  
	private String desinger; // 设计师  
	private String ballimg; //[ 图片1,图片2 ,图片3,  图片4 ] 
	public String getBallimg() {
		return ballimg;
	}
	public void setBallimg(String ballimg) {
		this.ballimg = ballimg;
	}
	private String fairwaylen; // 球场长度  
	private String fwgtype; // 球道草种  
	private String grasstype; // 果岭草种  
	private String holetype; // 球洞分类  
	private String model; // 球场模式  (球场/练习场)
	private String phone; // 联系电话  
	private String related; // 相关配套：  
	private String summy; // 球场简介 
	public String status;
	public String info;
	private ArrayList<BallTagDetailEntity> data;
	
	public BallTagDetailEntity(){}
	public long getArea() {
		return area;
	}
	public String getBkdata() {
		return bkdata;
	}
	public String getCreatetime() {
		return createtime;
	}
	public String getDesinger() {
		return desinger;
	}
	public String getFairwaylen() {
		return fairwaylen;
	}
	public String getFwgtype() {
		return fwgtype;
	}
	public String getGrasstype() {
		return grasstype;
	}
	public String getHoletype() {
		return holetype;
	}
	public String getModel() {
		return model;
	}
	public String getPhone() {
		return phone;
	}
	public String getRelated() {
		return related;
	}
	public String getSummy() {
		return summy;
	}

	public void setArea(long area) {
		this.area = area;
	}
	 public void setBkdata(String bkdata) {
		this.bkdata = bkdata;
	}
	 public void setCreatetime(String createtime) {
		this.createtime = createtime;
	}
	 public void setDesinger(String desinger) {
		this.desinger = desinger;
	}
	 public void setFairwaylen(String fairwaylen) {
		this.fairwaylen = fairwaylen;
	}
	 public void setFwgtype(String fwgtype) {
		this.fwgtype = fwgtype;
	}
	 public void setGrasstype(String grasstype) {
		this.grasstype = grasstype;
	}
	 public void setHoletype(String holetype) {
		this.holetype = holetype;
	}
	 public void setModel(String model) {
		this.model = model;
	}
	 public void setPhone(String string) {
		this.phone = string;
	}
	 public void setRelated(String related) {
		this.related = related;
	}
	 public void setSummy(String summy) {
		this.summy = summy;
	}
	public String getStatus() {
		return status;
	}
	public void setStatus(String status) {
		this.status = status;
	}
	public String getInfo() {
		return info;
	}
	public void setInfo(String info) {
		this.info = info;
	}
	public ArrayList<BallTagDetailEntity> getData() {
		return data;
	}
	public void setData(ArrayList<BallTagDetailEntity> data) {
		this.data = data;
	}


}
