package me.wangolf.bean.college;

import java.util.ArrayList;

import me.wangolf.bean.shop.PageInfo;

public class Basebean {
    private String status;
    private String info;
    private ArrayList<KnowledgeEntity> data;
    private ArrayList<PageInfo> pageinfo;

    public String getStatus() {
        return status;
    }

    public void setStatus(String status) {
        this.status = status;
    }

    public String getInfo() {
        return info;
    }

    public void setInfo(String info) {
        this.info = info;
    }

    public ArrayList<KnowledgeEntity> getData() {
        return data;
    }

    public void setData(ArrayList<KnowledgeEntity> data) {
        this.data = data;
    }

    public ArrayList<PageInfo> getPageinfo() {
        return pageinfo;
    }

    public void setPageinfo(ArrayList<PageInfo> pageinfo) {
        this.pageinfo = pageinfo;
    }

}
