import java.io.Serializable;

public class BallDistriEntity implements Serializable {

	private static final long serialVersionUID = 1L;

	private String dname;// 分销商名称
	private int id;// 球场供应商发布的球场id
	private int cid;// 供应商id
	private int price; // 价格
	private String service; // 提供的服务(18洞果岭/僮/车/柜)
	private String book;// 预定说明
	private String cancelbook;// 预定说明
	private String preprice;// 2630”//实际支付价格
	private int pricetype;// 类型

	public String getPreprice() {
		return preprice;
	}

	public int getPricetype() {
		return pricetype;
	}

	public void setPricetype(int pricetype) {
		this.pricetype = pricetype;
	}

	public void setPreprice(String preprice) {
		this.preprice = preprice;
	}

	public int getCid() {
		return cid;
	}

	public void setCid(int cid) {
		this.cid = cid;
	}

	public String getBook() {
		return book;
	}

	public void setBook(String book) {
		this.book = book;
	}

	public String getCancelbook() {
		return cancelbook;
	}

	public void setCancelbook(String cancelbook) {
		this.cancelbook = cancelbook;
	}

	public BallDistriEntity() {
	}

	public String getDname() {
		return dname;
	}

	public int getId() {
		return id;
	}

	public int getPrice() {
		return price;
	}

	public String getService() {
		return service;
	}

	public void setDname(String dname) {
		this.dname = dname;
	}

	public void setId(int id) {
		this.id = id;
	}

	public void setPrice(int price) {
		this.price = price;
	}

	public void setService(String service) {
		this.service = service;
	}

}
