public class CommodityEntity implements Serializable {
    private String status;
    private String info;
    private int id;
    private ArrayList<CommodityEntity> data;
    private String name;

    public String getStatus() {
        return status;
    }

    public void setStatus(String status) {
        this.status = status;
    }

    public String getInfo() {
        return info;
    }

    public void setInfo(String info) {
        this.info = info;
    }

    public int getId() {
        return id;
    }

    public void setId(int id) {
        this.id = id;
    }

    public ArrayList<CommodityEntity> getData() {
        return data;
    }

    public void setData(ArrayList<CommodityEntity> data) {
        this.data = data;
    }

    public String getName() {
        return name;
    }

    public void setName(String name) {
        this.name = name;
    }
}
