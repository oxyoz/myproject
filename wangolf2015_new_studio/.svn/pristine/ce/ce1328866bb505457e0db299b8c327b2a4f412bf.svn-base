<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/LinearLayout1"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical" >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical" >

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="200dp"
                android:layout_marginTop="-3dp" >

                <ImageView
                    android:id="@+id/ball_img"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/layout_height_200"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentTop="true"
                    android:layout_marginLeft="-2dp"
                    android:clickable="true"
                    android:scaleType="centerCrop"
                    android:src="@drawable/main_play_logo" />

                <RelativeLayout
                    android:id="@+id/relayoutweather"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_marginTop="@dimen/layout_height_8"
                    android:background="@drawable/info_bg_tianqi"
                    android:clickable="true">

                    <LinearLayout
                        android:id="@+id/layout_towheather"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:gravity="center"
                        android:orientation="horizontal" >

                        <TextView
                            android:id="@+id/tv_weather"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="@dimen/layout_height_8"
                            android:text=""
                            android:textColor="@color/white"
                            android:textSize="10sp" />

                        <ImageView
                            android:id="@+id/img_whether"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:scaleType="fitCenter" />
                    </LinearLayout>
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/head_layout_nagtive"
                    android:layout_width="45dp"
                    android:layout_height="45dp"
                    android:layout_alignParentBottom="true"
                    android:layout_alignParentLeft="true"
                    android:layout_marginBottom="8dp"
                    android:layout_marginLeft="8dp"
                    android:background="@drawable/bg_round_black_trans"
                    android:clickable="true"
                    android:gravity="center"
                    android:padding="5dp" >

                    <ImageView
                        android:id="@+id/imageView1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentTop="true"
                        android:layout_centerHorizontal="true"
                        android:src="@drawable/dibiao_icon" />

                    <TextView
                        android:id="@+id/head_tv_nagtive"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/imageView1"
                        android:layout_centerHorizontal="true"
                        android:gravity="center"
                        android:text="导航"
                        android:textColor="@color/white"
                        android:textSize="@dimen/s_font" />
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/head_layout_detail"
                    android:layout_width="45dp"
                    android:layout_height="45dp"
                    android:layout_alignTop="@+id/head_layout_nagtive"
                    android:layout_marginLeft="10dp"
                    android:layout_toRightOf="@+id/head_layout_nagtive"
                    android:background="@drawable/bg_round_black_trans"
                    android:clickable="true"
                    android:gravity="center"
                    android:padding="5dp" >

                    <ImageView
                        android:id="@+id/ImageView03"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerHorizontal="true"
                        android:src="@drawable/detail_icon" />

                    <TextView
                        android:id="@+id/head_tv_detail"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/ImageView03"
                        android:layout_centerHorizontal="true"
                        android:gravity="center"
                        android:text="详情"
                        android:textColor="@color/white"
                        android:textSize="@dimen/s_font" />
                </RelativeLayout>
            </RelativeLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/layout_height_8"
            android:background="@drawable/textlines_all"
            android:orientation="vertical" >

            <RelativeLayout
                android:id="@+id/RelativeLayout02"
                android:layout_width="match_parent"
                android:layout_height="@dimen/layout_height_40"
                android:gravity="center_vertical" >

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="@dimen/layout_height_40"
                    android:layout_alignParentBottom="true"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentRight="true"
                    android:gravity="center_vertical" >

                    <RelativeLayout
                        android:id="@+id/layout_date"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_marginBottom="1px"
                        android:layout_marginLeft="1px"
                        android:layout_marginRight="1px"
                        android:layout_marginTop="1px"
                        android:layout_weight="1"
                        android:background="@drawable/gray_light_selector"
                        android:clickable="true"
                        android:gravity="center_vertical"
                        android:orientation="horizontal" >

                        <TextView
                            android:id="@+id/imageView4"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerVertical="true"
                            android:layout_marginLeft="@dimen/layout_height_8"
                            android:text="日期："
                            android:textColor="@color/common_text"
                            android:textSize="@dimen/m_font" />

                        <TextView
                            android:id="@+id/TextView05"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerVertical="true"
                            android:layout_toRightOf="@+id/imageView4"
                            android:text=""
                            android:textSize="16sp" />

                        <TextView
                            android:id="@+id/tv_date"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerVertical="true"
                            android:layout_toRightOf="@+id/TextView05"
                            android:text="4月6日"
                            android:textAppearance="?android:attr/textAppearanceMedium"
                            android:textColor="@color/gray"
                            android:textSize="12sp" />
                    </RelativeLayout>

                    <View
                        android:id="@+id/view1"
                        android:layout_width="1px"
                        android:layout_height="match_parent"
                        android:layout_margin="3dp"
                        android:background="#999999" />

                    <RelativeLayout
                        android:id="@+id/layout_time"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_marginBottom="1px"
                        android:layout_marginLeft="1px"
                        android:layout_marginRight="1px"
                        android:layout_marginTop="1px"
                        android:layout_weight="1"
                        android:background="@drawable/gray_light_selector"
                        android:clickable="true"
                        android:gravity="center_vertical"
                        android:orientation="horizontal" >

                        <TextView
                            android:id="@+id/imageView5"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerVertical="true"
                            android:layout_marginLeft="@dimen/layout_height_8"
                            android:text="时间："
                            android:textColor="@color/common_text"
                            android:textSize="@dimen/m_font" />

                        <TextView
                            android:id="@+id/tv_time"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerVertical="true"
                            android:layout_toRightOf="@id/TextView04"
                            android:text="3:00"
                            android:textAppearance="?android:attr/textAppearanceMedium"
                            android:textColor="@color/gray"
                            android:textSize="12sp" />

                        <TextView
                            android:id="@+id/TextView04"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerVertical="true"
                            android:layout_toRightOf="@+id/imageView5"
                            android:text=""
                            android:textSize="16sp" />
                    </RelativeLayout>
                </LinearLayout>
            </RelativeLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/layout_height_8"
            android:layout_marginRight="@dimen/layout_height_8"
            android:orientation="vertical" >

            <LinearLayout
                android:id="@+id/a_layout_youhui"
                android:layout_width="match_parent"
                android:layout_height="@dimen/layout_height_50"
                android:layout_marginTop="@dimen/layout_height_8"
                android:background="@drawable/textlines_all"
                android:clickable="true"
                android:gravity="center"
                android:visibility="gone" >

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_margin="1dp"
                    android:background="@drawable/gray_light_selector"
                    android:gravity="center_vertical" >

                    <TextView
                        android:id="@+id/a_tv_youhui"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="10dp"
                        android:layout_weight="1"
                        android:text="优惠时段"
                        android:textSize="@dimen/m_font" />

                    <TextView
                        android:id="@+id/a_tv_youhui_time"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginRight="10dp"
                        android:layout_weight="1"
                        android:gravity="right"
                        android:text=""
                        android:textColor="@color/txt_orage"
                        android:textSize="@dimen/m_font" />
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:id="@+id/b_layout_taocan"
                android:layout_width="match_parent"
                android:layout_height="@dimen/layout_height_50"
                android:layout_marginTop="@dimen/layout_height_8"
                android:background="@drawable/textlines_all"
                android:clickable="true"
                android:gravity="center"
                android:visibility="gone" >

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_margin="1dp"
                    android:background="@drawable/gray_light_selector"
                    android:gravity="center" >

                    <TextView
                        android:id="@+id/b_tv_taocan"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="套餐"
                        android:textSize="@dimen/m_font" />
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:id="@+id/c_layout_all"
                android:layout_width="match_parent"
                android:layout_height="@dimen/layout_height_50"
                android:layout_marginTop="@dimen/layout_height_8"
                android:background="@drawable/ball_type_swit"
                android:gravity="center"
                android:visibility="gone" >

                <TextView
                    android:id="@+id/c_tv_all_youhui"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_margin="1dp"
                    android:layout_weight="1"
                    android:background="@drawable/gray_light_selector"
                    android:clickable="true"
                    android:gravity="center"
                    android:text="优惠时段"
                    android:textSize="@dimen/m_font" />

                <TextView
                    android:id="@+id/c_tv_all_taocan"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_margin="1dp"
                    android:layout_weight="1"
                    android:background="@drawable/gray_light_selector"
                    android:clickable="true"
                    android:gravity="center"
                    android:text="套餐"
                    android:textSize="@dimen/m_font" />
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/layout_height_8" >
        </LinearLayout>

        <View
            android:id="@+id/daili_borld"
            android:layout_width="match_parent"
            android:layout_height="1px"
            android:background="#999999"
            android:visibility="gone" >
        </View>
    </LinearLayout>

</LinearLayout>