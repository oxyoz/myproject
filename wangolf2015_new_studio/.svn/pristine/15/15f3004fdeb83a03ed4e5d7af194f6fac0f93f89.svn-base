package me.wangolf.bean.shop;

public class PageInfo {
	private String pageIndex;
	private String pageSize;
	private String totaPages;
	private String totaRecords;
	public String getPageIndex() {
		return pageIndex;
	}
	public void setPageIndex(String pageIndex) {
		this.pageIndex = pageIndex;
	}
	public String getPageSize() {
		return pageSize;
	}
	public void setPageSize(String pageSize) {
		this.pageSize = pageSize;
	}
	public String getTotaPages() {
		return totaPages;
	}
	public void setTotaPages(String totaPages) {
		this.totaPages = totaPages;
	}
	public String getTotaRecords() {
		return totaRecords;
	}
	public void setTotaRecords(String totaRecords) {
		this.totaRecords = totaRecords;
	}

}
