<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/LinearLayout1"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    android:background="@drawable/registe_all_round"
  android:gravity="center_horizontal"
     >
<ImageView
    android:layout_marginTop="10dp"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:src="@drawable/regist_back_title"
    />

<TextView
    android:id="@+id/tv_info"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
android:layout_marginTop="10dp"
    android:layout_marginBottom="10dp"
    android:textColor="#b2201f"
    android:text=""/>

        <TextView
            android:id="@+id/tv_wwnprice"
            android:background="@drawable/regist_back_mid"
            android:layout_centerInParent="true"
            android:text=""
            android:gravity="center"
            android:paddingLeft="40dp"
            android:textColor="#b2201f"
            android:textStyle="bold"
            android:textSize="30dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />


    <TextView
        android:layout_marginTop="10dp"
        android:layout_marginBottom="10dp"
        android:id="@+id/tv_otherinfo"
        android:gravity="center"
        android:textColor="#b2201f"
        android:text=""
        android:layout_width="200dp"
        android:layout_height="wrap_content"></TextView>

    <Button
        android:background="@drawable/but_all_round"
        android:id="@+id/but_enter"
        android:layout_width="wrap_content"
        android:text="我知道了"
        android:textColor="@color/white"
        android:textSize="20dp"
       android:paddingLeft="@dimen/layout_height_20"
        android:paddingRight="@dimen/layout_height_20"
        android:paddingTop="5dp"
        android:paddingBottom="5dp"
        android:layout_height="wrap_content" />
    <LinearLayout
        android:layout_marginBottom="@dimen/layout_height_10"
        android:layout_width="290dp"
        android:layout_height="wrap_content"></LinearLayout>
</LinearLayout>